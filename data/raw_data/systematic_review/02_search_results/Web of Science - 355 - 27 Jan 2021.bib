@article{ ISI:000232135300021,
Author = {Groothuis, TGG and Eising, CM and Dijkstra, C and Muller, W},
Title = {{Balancing between costs and benefits of maternal hormone deposition in
   avian eggs}},
Journal = {{BIOLOGY LETTERS}},
Year = {{2005}},
Volume = {{1}},
Number = {{1}},
Pages = {{78-81}},
Month = {{MAR}},
Abstract = {{Avian eggs contain substantial amounts of maternal androgens, and
   several studies have indicated that these are beneficial for the chick.
   Nevertheless, there is a large and systematic variation in maternal
   hormone concentrations both within and between clutches. If maternal
   androgens also involve costs, this might explain why not all mothers put
   high levels of androgens in their clutches. However, the, simultaneous
   occurrence of both benefits and costs has not yet been convincingly
   demonstrated. We show experimentally that yolk androgens suppress immune
   function and simultaneously stimulate growth in black-headed gull
   chicks. Thus, mothers face a trade-off between these costs and benefits
   and may tune hormone deposition to prevailing conditions that influence
   chick survival.}},
