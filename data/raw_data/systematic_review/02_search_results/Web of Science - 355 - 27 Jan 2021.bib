@article{ ISI:000394106400018,
Author = {Parolini, Marco and Romano, Andrea and Possenti, Cristina Daniela and
   Caprioli, Manuela and Rubolini, Diego and Saino, Nicola},
Title = {{Contrasting effects of increased yolk testosterone content on
   development and oxidative status in gull embryos}},
Journal = {{JOURNAL OF EXPERIMENTAL BIOLOGY}},
Year = {{2017}},
Volume = {{220}},
Number = {{4}},
Pages = {{625-633}},
Month = {{FEB 15}},
Abstract = {{Hormone-mediated maternal effects generate variation in offspring
   phenotype. In birds, maternal egg testosterone (T) exerts differential
   effects on offspring traits after hatching, suggesting that mothers
   experience a trade-off between contrasting T effects. However, there is
   very little information on T pre-natal effects. In the yellow-legged
   gull (Larus michahellis), we increased yolk T concentration within
   physiological limits and measured the effects on development and
   oxidative status of late-stage embryos. T-treated embryos had a larger
   body size but a smaller brain than controls. Males had a larger brain
   than females, controlling for overall size. T treatment differentially
   affected brain mass and total amount of pro-oxidants in the brain
   depending on laying order. T-treatment effects were not sex dependent.
   For the first time in the wild, we show contrasting T prenatal effects
   on body mass and brain size. Hence, T may enforce trade-offs between
   different embryonic traits, but also within the same trait during
   different developmental periods.}},
